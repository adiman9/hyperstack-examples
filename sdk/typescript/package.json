{
  "name": "hyperstack-bags",
  "version": "0.1.0",
  "description": "Bags fees v2 stack for Hyperstack",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./bags": {
      "types": "./dist/bags/index.d.ts",
      "import": "./dist/bags/index.js",
      "require": "./dist/bags/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "hyperstack-typescript": ">=0.4.0"
  },
  "peerDependenciesMeta": {
    "hyperstack-typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "hyperstack",
    "solana",
    "blockchain",
    "streaming",
    "bags"
  ],
  "author": "HyperTek",
  "license": "MIT"
}
